{
	"name": "mock-service-plugin",
	"version": "1.8.1",
	"type": "module",
	"description": "Simulated backend interface service",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"start": "node lib/index.js",
		"dev": "tsup --watch",
		"build": "rimraf lib && tsup",
		"build:prod": "npm run build -- --minify",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest",
		"lint": "eslint . --ext .ts",
		"format": "prettier --write \"src/**/*.ts\"",
		"release": "node scripts/release.js"
	},
	"keywords": [
		"mock",
		"mockjs",
		"webpack-plugin",
		"mock service",
		"json server"
	],
	"author": {
		"name": "Gleason",
		"email": "bianliuzhu@gmail.com",
		"url": "https://github.com/bianliuzhu"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bianliuzhu/mock-service-plugin.git"
	},
	"homepage": "https://github.com/bianliuzhu/mock-service-plugin#readme",
	"bugs": {
		"url": "https://github.com/bianliuzhu/mock-service/issues",
		"email": "bianliuzhu@gmail.com"
	},
	"files": [
		"lib"
	],
	"license": "ISC",
	"dependencies": {
		"chalk": "^5.0.0",
		"csv-stringify": "^6.5.2",
		"express": "^4.18.2",
		"glob": "^11.0.3",
		"mockjs": "^1.1.0",
		"watch": "^1.0.2",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.23",
		"@types/mockjs": "^1.0.10",
		"@types/node": "^20.19.0",
		"@types/watch": "^1.0.6",
		"@types/xml2js": "^0.4.14",
		"rimraf": "^6.0.1",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3",
		"eslint": "^8.56.0",
		"prettier": "^3.2.4",
		"jest": "^29.7.0",
		"tsx": "^4.20.3"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
